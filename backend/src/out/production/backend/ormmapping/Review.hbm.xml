<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Review" table="Review" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="author" cascade="lock" column="UserID" class="_Costumer" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="classification" column="Classification" type="integer" length="10" not-null="true" lazy="false"/>
		<property name="comment" column="Comment" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
