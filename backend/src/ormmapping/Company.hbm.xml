<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Company" table="Company" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="name" column="Name" type="string" length="255" not-null="false" lazy="false"/>
		<property name="email" column="Email" type="string" length="255" not-null="false" lazy="false"/>
		<property name="contact" column="Contact" type="integer" length="10" not-null="true" lazy="false"/>
		<property name="address" column="Address" type="string" length="255" not-null="false" lazy="false"/>
		<property name="logoImage" column="LogoImage" type="string" length="255" not-null="false" lazy="false"/>
		<property name="postCode" column="PostCode" type="string" length="255" not-null="false" lazy="false"/>
		<property name="schedule" column="Schedule" type="string" length="255" not-null="false" lazy="false"/>
		<set name="ORM_SocialNetworks" lazy="true" cascade="save-update,lock" inverse="false">
			<key column="CompanyID" not-null="true"/>
			<one-to-many class="SocialNetwork"/>
		</set>
	</class>
</hibernate-mapping>
